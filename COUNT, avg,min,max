CREATE TABLE EMPLOYEE (
  empId INTEGER PRIMARY KEY,
  name TEXT NOT NULL,
  salary TEXT NOT NULL
);


INSERT INTO EMPLOYEE VALUES (0001, 'Clark', '10000');
INSERT INTO EMPLOYEE VALUES (0002, 'Dave', '20000');
INSERT INTO EMPLOYEE VALUES (0003, 'Ava', '5000');

 
SELECT * FROM EMPLOYEE WHERE salary = '5000';
SELECT COUNT (empid)
FROM EMPLOYEE
WHERE salary > 10000;

SELECT AVG(salary)
FROM EMPLOYEE;

select MIN(salary)
from EMPLOYEE;

SELECT MAX(salary)
FROM EMPLOYEE;